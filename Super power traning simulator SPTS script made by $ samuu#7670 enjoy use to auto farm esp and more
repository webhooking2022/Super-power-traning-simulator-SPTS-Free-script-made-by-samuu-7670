local clicker = {}
function getRoot(plr)
    plr = plr or game.Players.LocalPlayer

    local sucess, root = pcall(function()
        return plr.Character.HumanoidRootPart
    end)
    if sucess then return root end
end
function getHumanoid(plr)
    plr = plr or game.Players.LocalPlayer

    local sucess, Humanoid = pcall(function()
        return plr.Character.Humanoid
    end)
    if sucess then return Humanoid end
end
function round(num, numDecimalPlaces)
	local mult = 10^(numDecimalPlaces or 0)
	return math.floor(num * mult + 0.5) / mult
end
		
function converttoletter(num)
	if num / 1e21 >=1 then
		newnum = num / 1e21
		return round(newnum, 6).. "Sx"
	elseif num / 1e18 >=1 then
		newnum = num / 1e18
		return round(newnum, 6).. "Qi"
	elseif num / 1e15 >=1 then
		newnum = num / 1e15
		return round(newnum, 6).. "Qa"
	elseif num / 1e12 >=1 then
		newnum = num / 1e12
		return round(newnum, 6).. "T"
	elseif num / 1e09 >=1 then
		newnum = num / 1e09
		return round(newnum, 6).. "B"
	elseif num / 1e06 >=1 then
		newnum = num / 1e06
		return round(newnum, 6).. "M"
	elseif num / 1e03 >=1 then
		newnum = num / 1e03
		return round(newnum, 6).. "K"
	else return num
	end
end


Translate = function(num)
    return converttoletter(num)
end

function AutoClick(bool)
    if bool then
        clicker[#clicker + 1] = game:GetService("RunService").Stepped:Connect(function()
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v:IsA("Tool") then
                    v:Activate()
                end
            end
        end)
    else
        if clicker then
            for i,v in pairs(clicker) do
                v:Disconnect()
            end
        end
    end
    
end

local Stats = {
    First = game:GetService("Players").LocalPlayer.PrivateStats.FistStrength;
    Body = game:GetService("Players").LocalPlayer.PrivateStats.BodyToughness;
    Psy = game:GetService("Players").LocalPlayer.PrivateStats.PsychicPower;
}

function Equip(name)
    pcall(function()
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v.Name == name then
                v.Parent = game.Players.LocalPlayer.Character
            end
        end
    end)
end

function NewFarm(farm_name, toolname, cframes)
    
    local oldpos
    local myfarm
    farms:CreateToggle(farm_name, function(bool)
        AutoClick(bool)
        if bool then
            oldpos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            myfarm = game:GetService("RunService").Heartbeat:Connect(function()
                local pos

                for i,v in pairs(cframes) do
                    if v.stats.Value >= v.number  then
                        pos = v.cframe
                        if getgenv().safefarm == true then
                            pos = pos + Vector3.new(0, -10, 0)
                        end
                    end
                end

                Equip(toolname)
                
                
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
            end)
        else
            pcall(function()
                for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                    if v:IsA("Tool") then
                        v.Parent = game.Players.LocalPlayer.Backpack
                    end
                end
            end)
            myfarm:Disconnect()
            for i = 1,10 do
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldpos
                wait()
                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
            end
        end
    end)
end
local autorespawn = false
function Refresh()
    local speaker = game.Players.LocalPlayer

    local Root = speaker.Character:WaitForChild("HumanoidRootPart") or speaker.CharacterAdded:wait():WaitForChild("HumanoidRootPart")
    
    local old = Root.CFrame
    Root.CFrame = CFrame.new(599, 999, 900)
    game:GetService("ReplicatedStorage").RemoteEvent:FireServer({[1] = "Respawn"})
    
    speaker.CharacterAdded:wait()
    local speaker = game.Players.LocalPlayer
    local Root = speaker.Character:WaitForChild("HumanoidRootPart") or speaker.CharacterAdded:wait():WaitForChild("HumanoidRootPart")
    for i = 1,10 do

        Root.CFrame = old
        wait(0.1)
    end
end

farms:CreateButton("Refresh", Refresh)
function Unequip_All()
    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if v:IsA("Tool") then
            v.Parent = game.Players.LocalPlayer.Backpack
        end
    end
end
farms:CreateToggle("Auto respawn", function(bool)
    autorespawn = bool 

    spawn(function()
        while autorespawn do
            pcall(function()
                if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
                    Refresh()
                end
            end)
            wait(1)
        end
    end)
end)

