    VisibleEsp = bool
end)
esp:CreateToggle("Line Enabled", function(bool)
    VisibleLine = bool
end)

getgenv().espmax = false
esp:CreateToggle("Esp max 150", function(bool)
    getgenv().espmax = bool
end)

