function toggleJumpPower(bool)
    jumppower = bool
    for i = 1,10 do
        task.spawn(function()
            while jumppower and task.wait() do
                pcall(function()
                    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
                end)
            end
        end)
    end
end
function getHeight()

    if game.Players.LocalPlayer.Character:FindFirstChild("RightWeight4") and game.Players.LocalPlayer.Character:FindFirstChild("LeftWeight4") then

            return true

    end

    return false

end



