    end)
end)

aimbot:CreateToggle("Kill With First", function(b)
    kill = b 

    spawn(function()
        if kill then
            oldpos = game.Players.LocalPlayer.Character.Head.Position
        end
        while kill and task.wait() do
            local args = {
                [1] = {
                    [1] = "Skill_BulletPunch",
                    [2] = "Right",
                    [3] = target.Character.Head.Position
                }
            }
            
            
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
            local args = {
                [1] = {
                    [1] = "Skill_SpherePunch",
                    [2] = target.Character.Head.Position
                }
            }
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
            speaker.Character.HumanoidRootPart.CFrame = CFrame.new(target.Character.Head.Position + Vector3.new(0, 10, 0))

        end
    end)

    if not kill then
        for i = 1,10 do
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(oldpos)
            wait()
        end
    end
end)

