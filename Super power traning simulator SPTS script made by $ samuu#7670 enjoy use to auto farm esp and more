    function getBest(tab)
        local pos, tool, location
        for i,v in pairs(tab) do
            if typeof(v) == "table" and v.stats.Value > v.number then
                pos = v.cframe
                location = i
                tool = tab[1]
            end
        end
        return pos, tool, location
    end
    zero = Vector3.new(0,0,0)
    AutoClick(three)
    if bool then
        oldpos = getRoot().CFrame
        spawn(function()
            while three and task.wait() do
                pcall(function()
                    if psy_active and three then
                        for i = 0,times_spy do
                            local pos, toolname = getBest(Psy)
                            if pos and toolname then
                                Unequip_All()
                                Equip(toolname)
                                local reached = false
                                connection = game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.ChildAdded:Connect(function(child)
                                    if not (child.Name ~= "MSXpFrame" and child.Name == "JFXpFrame") then
                                        reached = true
                                    end
                                end)
                                repeat
                                    Equip(toolname)
                                    getRoot().CFrame = pos 
                                    getRoot().Velocity = zero
                                    task.wait()
                                until reached
                                connection:Disconnect()
                            end
                        end
                    end
                    if body_active and three then
                        for i = 0,times_body do
                            local pos, toolname, location = getBest(Body)
    
                            if pos and toolname then
                                Unequip_All()
  
                                local reached = false
                                connection = game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.ChildAdded:Connect(function(child)
                                    if not (child.Name ~= "MSXpFrame" and child.Name == "JFXpFrame") then
                                        reached = true
                                    end
                                end)
                                repeat

                                    getRoot().CFrame = pos  Equip(toolname)
                                    getRoot().Velocity = zero
                                    task.wait()
                                until reached
                                connection:Disconnect()
                            end
                        end
                    end
                    if first_active and three then
                        for i = 0,times_first do
                            local pos, toolname, location = getBest(First)
    
                            if pos and toolname then
                                Unequip_All()
                                Equip(toolname)
                                AutoClick(false)
                                delay(0.2, function()
                                    AutoClick(true)
                                end)
        
                                local reached = false
                                connection = game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui.ChildAdded:Connect(function(child)
                                    if not (child.Name ~= "MSXpFrame" and child.Name == "JFXpFrame") then
                                        reached = true
                                    end
                                end)
                                repeat
                                    getRoot().CFrame = pos 
                                    getRoot().Velocity = zero
                                    Equip(toolname)
                                    
                                    task.wait()
                                until reached
                                connection:Disconnect()
                            end
                        end
                    end
                end)
            end
            
        end)
        spawn(function()
            toggleJumpPower(true)
            while three and task.wait() do
                if jump_active == true then
                    
                    game.Players.LocalPlayer.Character.Humanoid.Jump = true
                    if not getHeight() then
                        local args = {
                            [1] = {
                                [1] = "Weight",
                                [2] = "Weight4"
                            }
                        }
                        
                        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
                        wait(1)                        
                    end
                end
            end
            
        end)
    else
        for i = 1,15 do
            toggleJumpPower(false)
            getRoot().CFrame = oldpos
            wait()
        end
    end
end)

