auto_refresh:CreateButton("How much of health i have? (Click here)", function()
    local msg = Instance.new("Message", game.CoreGui)
    msg.Text = "You have "..Translate(game.Players.LocalPlayer.Character.Humanoid.Health) .. ' of health.\nI recommend your put: '.. Translate((game.Players.LocalPlayer.Character.Humanoid.Health / 5));
    game.Debris:AddItem(msg, 2)
end)

auto_refresh:CreateToggle("Active", function(bool)
    autorefresh = bool
    if bool then
        spawn(function()
            while autorefresh == true do
                repeat wait() until game.Players.LocalPlayer.Character.Humanoid.Health <= minHealth or autorefresh == false
                if autorefresh then Refresh() end
            end
        end)
    end
end)

local reputation = menu:CreateTab("Reputation")
local whatisfarm = false
function getReputationToVillain()
    local player

    for i,v in pairs(game.Players:GetChildren()) do

        if v ~= speaker and game:GetService("Players")[v.Name].PrivateStats.Reputation.Value >= 0 and not v.Character:FindFirstChild("SafeZoneShield") then

            local account = game:GetService("Players")[v.Name].PrivateStats.PsychicPower.Value < (speaker.PrivateStats.PsychicPower.Value / 5)

            if account == true then

                player = v

            end

        end

    end

    return player
end

function getReputationToProtector()
    local player

    for i,v in pairs(game.Players:GetChildren()) do

        if v ~= speaker and game:GetService("Players")[v.Name].PrivateStats.Reputation.Value < 0 and not v.Character:FindFirstChild("SafeZoneShield") then

            local account = game:GetService("Players")[v.Name].PrivateStats.PsychicPower.Value < (speaker.PrivateStats.PsychicPower.Value / 3)

            if account == true then

                player = v

            end

        end

    end

    return player
end


local whatisfarm = true
reputation:CreateToggle("Farm reputation", function(bool)
    reputation_farm = bool

    if bool then
        oldpos = functions.getRoot().CFrame 
        spawn(function()
    
            while reputation_farm do
    
                pcall(function()
                    local player = (whatisfarm == true and getReputationToVillain()) or (whatisfarm == false and getReputationToProtector())

                    if player and (functions.getHumanoid(player) and functions.getHumanoid(player).Health > 0) then 
                        functions.getRoot().CFrame = functions.getRoot(player).CFrame * CFrame.new(0, 10, 0)
                        local args = {
                            [1] = {
                                [1] = "Skill_SoulAttack_Start",
                                [2] = target
                            }
                        }
                        
                        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
                    end
                end)

                task.wait()

            end
    
        end)

    else
        
        functions.getRoot().CFrame = oldpos
        game:GetService("ReplicatedStorage").RemoteEvent:FireServer({"Skill_SoulAttack_Stop"})

    end
end)

reputation:CreateToggle("No reputation good?", function(bool)
    whatisfarm = bool
end)

local webhook = menu:CreateTab("Web hook")

local input_webhook = ''
local input_first = false
local input_body = false
local input_psychic = false
local input_delay = 1
webhook:CreateTextBox("Webhook", function(text)
    input_webhook = text 
end)

webhook:CreateTextBox("Delay web hook", function(text)
    input_delay = tonumber(text) 
end)



webhook:CreateToggle("Fist Stats", function(bool)
    input_first = bool
end)

webhook:CreateToggle("Body Stats", function(bool)
    input_body = bool
end)

webhook:CreateToggle("Psychic Stats", function(bool)
    input_psychic = bool
end)



webhook:CreateToggle("Start send webhooks", function(bool)
    webhook_spam = bool

    spawn(function()
        while webhook_spam do
            local data = os.date("!*t", tick())

            local final = string.format("**Dia: %s, Horario: %s:%s:%s**", data.day, data.hour, data.min, data.sec)

            local tab = {
                
                content = final,
                username = "SPTS Stats", avatar_url = "https://tr.rbxcdn.com/4e4b55732d99c773c46a097f66390d4b/768/432/Image/Png",
                embeds = {
                    {
                        title = "🎃SPTS🎃";
                        description = "Feito por $ samuu#7670";
                        color = tonumber(0x00ff00);

                        fields = {
                            
                        }

                    }
                }
            }

            local actives = {Psy = input_psychic, Body = input_body, First = input_first}

            for i,v in pairs(actives) do
                if v then
                    local name 

                    if i == "First" then
                        name = functions.Translate(Stats.First.Value)
                    end
                    if i == "Body" then
                        name = functions.Translate(Stats.Body.Value)
                    end
                    if i == "Psy" then
                        name = functions.Translate(Stats.Psy.Value)
                    end

                    name = string.format("**%s**", name)
                    
                    tab.embeds[1].fields[#tab.embeds[1].fields + 1] = {
                        name = i,
                        value = name,
                        inline = true,
                    }
                end
            end

            functions.sendWebHook(tab,input_webhook)
            wait(input_delay)
        end
    end)
end)

local FE = menu:CreateTab("FE")

FE:CreateToggle("Invisible", function(bool)
    if bool then
        oldpos = functions.getRoot().CFrame
        functions.getRoot().CFrame = CFrame.new(-4160, 241, -3508)
        wait(0.3)
        functions.getCharacter().LowerTorso:Destroy()
        wait(0.3)
        functions.getRoot().CFrame = oldpos 
    else
        Refresh()
    end
end)

FE:CreateButton("Split body (Deixar o torso)", function()
    local plrchr = game:GetService('Players').LocalPlayer.Character;
    plrchr.UpperTorso.Waist:Remove()

end)

FE:CreateButton("Split body (Sumir com o torso", function()
    local plrchr = game:GetService('Players').LocalPlayer.Character;

    oldpos = plrchr.HumanoidRootPart.CFrame
    plrchr.HumanoidRootPart.CFrame = CFrame.new(0, 999, 0)
    wait(0.5)
    plrchr.UpperTorso.Waist:Destroy()
    wait(0.3)
    plrchr.HumanoidRootPart.CFrame = oldpos
end)



local Others = menu:CreateTab("Others")


Others:CreateButton("Show fps", function()
    local Menu = Instance.new("ScreenGui", game.CoreGui)
    Menu.Name = "FPS"

    local TextLabel = Instance.new("TextLabel", Menu)
    TextLabel.BackgroundTransparency = 1

    TextLabel.TextColor3 = Color3.fromRGB(255,255,255)
    TextLabel.Text = "FPS: 0"

    TextLabel.Size = UDim2.new(0, 70, 0, 50)

    TextLabel.AnchorPoint = Vector2.new(1, 0)

    TextLabel.Position = UDim2.new(1,0,0,0)

    TextLabel.TextScaled = true


    local FPS = 0


    game:GetService("RunService").Stepped:Connect(function()

    FPS+=1

    end)


    spawn(function()

    while wait(1) do

    TextLabel.Text = "FPS: "..FPS

    FPS = 0 

    end

    end)
end)

Others:CreateButton("Rejoin Server", function()
    game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId, game.Players.LocalPlayer)
end)

Others:CreateButton("FPS boost", function()
    local Terrain = workspace:FindFirstChildOfClass('Terrain')
    local Lighting = game.Lighting
    local RunService = game:GetService("RunService")
	Terrain.WaterWaveSize = 0
	Terrain.WaterWaveSpeed = 0
	Terrain.WaterReflectance = 0
	Terrain.WaterTransparency = 0
	Lighting.GlobalShadows = false
	Lighting.FogEnd = 9e9
	settings().Rendering.QualityLevel = 1
	for i,v in pairs(game:GetDescendants()) do
		if v:IsA("Part") or v:IsA("UnionOperation") or v:IsA("MeshPart") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
			v.Material = "Plastic"
			v.Reflectance = 0
		elseif v:IsA("Decal") then
			v.Transparency = 1
		elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
			v.Lifetime = NumberRange.new(0)
		elseif v:IsA("Explosion") then
			v.BlastPressure = 1
			v.BlastRadius = 1
		end
	end
	for i,v in pairs(Lighting:GetDescendants()) do
		if v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("ColorCorrectionEffect") or v:IsA("BloomEffect") or v:IsA("DepthOfFieldEffect") then
			v.Enabled = false
		end
	end
	workspace.DescendantAdded:Connect(function(child)
		coroutine.wrap(function()
			if child:IsA('ForceField') then
				RunService.Heartbeat:Wait()
				child:Destroy()
			elseif child:IsA('Sparkles') then
				RunService.Heartbeat:Wait()
				child:Destroy()
			elseif child:IsA('Smoke') or child:IsA('Fire') then
				RunService.Heartbeat:Wait()
				child:Destroy()
			end
		end)()
	end)
end)

