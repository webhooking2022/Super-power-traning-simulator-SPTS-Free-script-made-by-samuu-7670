function getPlayer(name)
    for i,v in pairs(game.Players:GetChildren()) do
        if v.Name:lower():match(name:lower()) then
            return v 
        end
    end
end
local target
local Lock = false

local gmt = getrawmetatable(game)
local namecall = gmt.__namecall

setreadonly(gmt, false)

gmt.__namecall = newcclosure(function(self, ...)
    local args = {...}
    local method = getnamecallmethod()

    if tostring(method) == "FireServer" and tostring(self) == "RemoteEvent" and Lock == true then

        pcall(function()
        
            if tostring(args[1]) == "Skill_BulletPunch" then

                args[3] = target.Character.Head.Position
    

            elseif tostring(args[1]) == "Skill_SpherePunch" then
    
                args[2] = target.Character.Head.Position
    

            end

        end)

        
        return self.FireServer(self, unpack(args))
    end

    return namecall(self, unpack(args))
end)

setreadonly(gmt, true)

