            speaker.Character.HumanoidRootPart.CFrame = CFrame.new(target.Character.Head.Position + Vector3.new(0, 10, 0))

        end
    end)

    if not kill then
        local args = {
            [1] = {
                [1] = "Skill_SoulAttack_End"
